
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <!-- Vendor CSS Files -->
    <link href="~/Content/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/quill/quill.snow.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/quill/quill.bubble.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/simple-datatables/style.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/Content/assets/css/style.css" rel="stylesheet">

    <meta name="viewport" content="width=device-width" />
    <title>StartExam</title>
</head>
<body>
    <div class="card bg-dark-light ">


        @*@Request.QueryString["ExamTitle"]
            @Request.QueryString["Exam_Id"]*@


        <input type="hidden" class="form-control" id="Student_Id" name="Student_Id" value="@Session["Student_Id"]">
        <input type="hidden" class="form-control" id="Exam_Id" name="Exam_Id" value="@Request.QueryString["Exam_Id"]">

        <div class="row">
            <div class="col-6 style">

                <span id="cast">Name:@Session["Student_Name"]</span>
            </div><br />
            <div class="col-6">
                <span id="">Father:@Session["Student_Father"]</span>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-6">
                <span id="">Mobile:@Session["Student_Mobile"]</span>
            </div><br />
            <div class="col-sm-6">
                <span id="">Email:@Session["Student_Email"]</span>
            </div>
            <div class="col-sm-6">
                <span id="">Exam: @Request.QueryString["ExamTitle"]</span>
            </div>
        </div><br /><br /><br />
        <div class="col-sm-5"></div>
        <div class="col-sm-2">
        
                <button class="btn btn-secondary" onclick="SaveHistory()">GetStart</button>
            
        </div>
        <div class="col-sm-5"></div>
    </div>

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/Content/assets/vendor/apexcharts/apexcharts.min.js"></script>
    <script src="~/Content/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/assets/vendor/chart.js/chart.min.js"></script>
    <script src="~/Content/assets/vendor/echarts/echarts.min.js"></script>
    <script src="~/Content/assets/vendor/quill/quill.min.js"></script>
    <script src="~/Content/assets/vendor/simple-datatables/simple-datatables.js"></script>
    <script src="~/Content/assets/vendor/tinymce/tinymce.min.js"></script>
    <script src="~/Content/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/Content/assets/js/main.js"></script>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>



    <script>

        function SaveHistory() {
            openhere();
            var Student_Id = $('#Student_Id').val();
            var Exam_Id = $('#Exam_Id').val();
            $.ajax({
                url: '/Current_Exam/Save_ExamHist',
                type: 'POST',
                data: { Student_Id: Student_Id, Exam_Id: Exam_Id},
                success: function (data) {
                    

                },
                error: function () {
                    alert("Success");
                }
            });
            
        }
        function openhere() {
            Que = window.open('/current_exam/examque', "status=yes,scrollbars=yes,resizable=no,type=fullWindow,maximized");
        }

    </script>
</body>
</html>

